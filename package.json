    "eslint": "^7.21.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^7.0.4",
    "jest": "^27.5.0",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.5.1",
    "release-it": "^14.12.4",
