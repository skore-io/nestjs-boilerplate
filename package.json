{
  "name": "nestjs-boilerplate",
  "version": "0.2.0",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint --ext .js,.ts ./",
    "prepare": "husky install",
    "start": "nest start",
    "start:debug": "NODE_PATH=. nest start --debug --watch",
    "start:dev": "NODE_PATH=. nest start --watch",
    "start:prod": "node dist/main",
    "test": "NODE_PATH=. jest --forceExit --detectOpenHandles",
    "test:unit": "NODE_PATH=. jest --config jest.config.unit.js --detectOpenHandles --runInBand",
    "test:int": "NODE_PATH=. jest --config jest.config.integration.js --detectOpenHandles --runInBand",
    "test:e2e": "NODE_PATH=. jest --config jest.config.e2e.js  --runInBand --forceExit"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.6",
    "@nestjs/axios": "^0.0.5",
    "@nestjs/common": "^8.4.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.4.0",
    "@nestjs/graphql": "^10.0.6",
    "@nestjs/platform-express": "^8.4.0",
    "@nestjs/typeorm": "^8.0.3",
    "@skore-io/auth": "^0.37.0",
    "apollo-server-core": "^3.6.2",
    "apollo-server-express": "^3.6.2",
    "class-transformer": "^0.5.1",
    "graphql": "^16.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "typeorm": "^0.2.43"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@nestjs/cli": "^8.2.1",
    "@nestjs/schematics": "^8.0.7",
    "@nestjs/testing": "^8.4.0",
    "@release-it/conventional-changelog": "^4.1.0",
    "@testdeck/jest": "^0.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.15",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.0",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.5.1",
    "release-it": "^14.12.5",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.6.2"
  },
  "jestSonar": {
    "reportFile": "test-reporter.xml",
    "indent": 2
  }
}
