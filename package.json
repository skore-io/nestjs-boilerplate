    "@skore-io/auth": "^0.37.0",
    "apollo-server-core": "^3.6.2",
    "apollo-server-express": "^3.6.2",
    "class-transformer": "^0.5.1",
    "graphql": "^15.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
