    "@nestjs/typeorm": "^10.0.1",
    "@skore-io/auth": "^1.19.2",
    "apollo-server-core": "^3.13.0",
    "apollo-server-express": "^3.13.0",
    "class-transformer": "^0.5.1",
    "graphql": "^16.8.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.19"
