  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint --ext .js,.ts ./",
    "prepare": "husky install",
    "start": "nest start",
    "start:debug": "NODE_PATH=. nest start --debug --watch",
    "start:dev": "NODE_PATH=. nest start --watch",
    "start:prod": "node dist/main",
    "test": "NODE_PATH=. jest --forceExit --detectOpenHandles"
  },
  "dependencies": {
